{% for flashtype,flash_messages in app.session.flashBag.all %}
    {% for flash_message in flash_messages %}
        <div class="row">
            <div class="col-md-12">
                <div class="note note-{{flashtype}}">

                    <p> 
                        {{ flash_message|trans }}
                    </p>
                </div>
            </div>
        </div>
    {% endfor %}
{% endfor %}